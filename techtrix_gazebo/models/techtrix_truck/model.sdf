<?xml version='1.0'?>
<sdf version='1.4'>
  <model name="techtrix_truck">
  	<static>true</static>
  	
  	<!-- We simply show the model, and for now we do not include any collisions -->
	<link name='truck_main'>
      	<!-- The collision box will have no z value, so that the environment is shown on the ground-->
		<collision name='collision'>
	      <geometry>
	        <mesh>
				<uri>model://techtrix_truck/meshes/model.dae</uri>
			</mesh>
	      </geometry>
	    </collision>

     <visual name='visual'>
       <geometry>
         <mesh>
         	<uri>model://techtrix_truck/meshes/model.dae</uri>
         </mesh>
       </geometry>
     </visual>
	</link>
  </model>
</sdf>